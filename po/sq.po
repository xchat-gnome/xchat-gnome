#: ../src/common/outbound.c:2226
msgid "Type /HELP <command> for more information, or /HELP -l"
msgstr "Për më tepër të dhëna, shtypni /HELP <urdhër>  ose /HELP -l"

#: ../src/common/outbound.c:2301
#, c-format
msgid "Unknown arg '%s' ignored."
